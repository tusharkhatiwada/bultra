{
  "expo": {
    "name": "Fast Growing",
    "slug": "fast-growing",
    "description": "Fast Growing description",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-app.png",
    "owner": "fastgrowing",
    "scheme": "fastgrowing",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.fastgrowing.app",
      "supportsTablet": true,
      "icon": "./src/assets/images/logo-app.png",
      "usesIcloudStorage": false,
      "associatedDomains": ["applinks:fastgrowing.com"]
    },
    "android": {
      "versionCode": 0,
      "package": "com.fastgrowing.app",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo-app.png",
        "backgroundColor": "#000000"
      },
      "splash": {
        "statusBar": {
          "visible": true,
          "barStyle": "dark-content"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.fastgrowing.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": ["expo-document-picker"]
  }
}
