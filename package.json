{
  "name": "mobile",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint --ext .ts,tsx --max-warnings 0 ./src",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collectCoverage",
    "prepare": "husky install",
    "expo-doctor": "node scripts/doctor.js",
    "g": "hygen generator",
    "eas:build-ios": "eas build --platform ios",
    "eas:submit_latest": "eas submit -p ios --latest"
  },
  "jest": {
    "preset": "jest-expo",
    "globalSetup": "<rootDir>/src/tests/global-setup.js",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts"
    ],
    "collectCoverageFrom": [
      "./src/**",
      "!./src/api/**",
      "!./src/assets/**",
      "!./src/config/**",
      "!./src/constants/**",
      "!./src/navigation/**"
    ]
  },
  "dependencies": {
    "@codler/react-native-keyboard-aware-scroll-view": "^2.0.1",
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@tanstack/react-query": "^4.0.10",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "expo": "~48.0.18",
    "expo-asset": "~8.9.1",
    "expo-barcode-scanner": "~12.3.2",
    "expo-camera": "~13.2.1",
    "expo-clipboard": "~4.1.2",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-document-picker": "~11.2.2",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-localization": "14.1.1",
    "expo-network": "~5.2.1",
    "expo-secure-store": "12.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "formik": "^2.2.9",
    "i18next": "^21.9.1",
    "lodash": "^4.17.21",
    "native-base": "^3.4.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^11.18.4",
    "react-native": "0.71.8",
    "react-native-phone-number-input": "^2.1.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "0.18.10",
    "react-qr-code": "^2.0.8",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^11.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.66.13",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "husky": "^8.0.1",
    "hygen": "^6.2.8",
    "jest": "^29.2.1",
    "jest-expo": "~45.0.0",
    "prettier": "2.3.2",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "~4.9.4"
  },
  "resolutions": {
    "@types/react": "18.0.27"
  },
  "private": true
}
